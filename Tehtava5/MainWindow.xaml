<Window x:Class="Tehtava5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tehtava5"
        mc:Ignorable="d"
        Title="MainWindow" Height="680" Width="525" FontSize="16">
    <Grid>
        <Button x:Name="buttonGetDataTable" Content="Hae läsnäolot (DataTable)" Click="buttonGetDataTable_Click" Margin="10,10,10,580"/>
        <DataGrid x:Name="dataGrid" Margin="10,255,10,10" ItemsSource="{Binding}"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="45" Margin="10,75,10,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="497"/>
        <Button x:Name="buttonGetDataView" Content="Hae läsnäoloni (DataView)" Click="buttonGetDataView_Click" Margin="10,125,10,465"/>
        <Button x:Name="buttonGetDataSet" Content="Hae läsnäolot (Typed DataSet)" Click="buttonGetDataSet_Click" Margin="10,190,10,400"/>
    </Grid>
</Window>
