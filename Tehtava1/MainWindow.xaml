        <Label x:Name="label6" Content="Karmin pinta-ala:" HorizontalAlignment="Right" Margin="0,268,371,0" VerticalAlignment="Top" RenderTransformOrigin="0.528,0.44"/>
        <Button x:Name="button" Content="Laske pinta-ala ja piiri" HorizontalAlignment="Left" Margin="37,108,0,0" VerticalAlignment="Top" Width="234" Height="43" Click="button_Click"/>

    </Grid>
