        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="23" Margin="151,49,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextAlignment="Center"/>
        <Label x:Name="label2" Content="Ikkunan Korkeus (H):" HorizontalAlignment="Right" Margin="0,46,371,0" VerticalAlignment="Top" RenderTransformOrigin="0.528,0.44"/>
        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Height="23" Margin="151,80,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Text="45" TextAlignment="Center"/>
        <Label x:Name="label3" Content="Karmipuun Leveys (W):" HorizontalAlignment="Right" Margin="0,77,371,0" VerticalAlignment="Top" RenderTransformOrigin="0.528,0.44"/>
